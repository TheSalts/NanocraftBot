{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\Captcha.js"
    },
    {
      "type": "node-terminal",
      "name": "Run Script: sr",
      "request": "launch",
      "command": "npm run sr",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node-terminal",
      "name": "Run Script: index",
      "request": "launch",
      "command": "node index.js",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node-terminal",
      "name": "Run Script: menu",
      "request": "launch",
      "command": "node menu.js",
      "cwd": "${workspaceFolder}"
    }
  ]
}
